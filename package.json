{
  "name": "appkeep",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node scripts/server.js",
    "preserve": "node scripts/credentials.js",
    "serve": "ng serve",
    "prebuild": "node scripts/credentials.js",
    "build": "ng build --aot --prod",
    "preserve:prod": "npm run build",
    "serve:prod": "http-server -p 4000 --proxy http://localhost:3000 dist/appkeep",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "postinstall": "npm run prebuild && ng build --aot --prod",
    "icons": "reload -b -s icons.html -e svg,html -d src/ -p 9000",
    "prepatterns": "sass src/styles.scss src/patterns.css -I . --no-source-map",
    "patterns": "reload -b -s patterns.html -e svg,css,html -d src/ -p 9001"
  },
  "engines": {
    "node": "~12.6.0",
    "npm": "~6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/pwa": "^0.803.16",
    "@angular/router": "~8.2.3",
    "@angular/service-worker": "~8.2.3",
    "angularx-social-login": "^2.1.1",
    "body-parser": "^1.19.0",
    "dexie": "^2.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "google-auth-library": "^5.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.2",
    "path": "^0.12.7",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "web-push": "^3.4.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "reload": "^3.0.3",
    "sass-planifolia": "^0.6.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "^3.5.3"
  }
}
