{
  "name": "appkeep",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node scripts/server.js",
    "preserve": "node scripts/credentials.js",
    "serve": "ng serve",
    "prebuild": "node scripts/credentials.js",
    "build": "ng build --aot --configuration production",
    "preserve:prod": "npm run build",
    "serve:prod": "http-server -p 4000 --proxy http://localhost:3000 dist/appkeep",
    "test": "ng test",
    "test:coverage": "ng test  --code-coverage",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage && codecov",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "icons": "reload -b -s icons.html -e svg,html -d src/ -p 9000",
    "prepatterns": "sass src/styles.scss src/patterns.css -I . --no-source-map",
    "patterns": "reload -b -s patterns.html -e svg,css,html -d src/ -p 9001"
  },
  "engines": {
    "node": "~12.6.0",
    "npm": "~6.9.0"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^2.0.0",
    "@angular/animations": "^15.2.3",
    "@angular/common": "^15.2.3",
    "@angular/compiler": "^15.2.3",
    "@angular/core": "^15.2.3",
    "@angular/forms": "^15.2.3",
    "@angular/platform-browser": "^15.2.3",
    "@angular/platform-browser-dynamic": "^15.2.3",
    "@angular/pwa": "^0.803.16",
    "@angular/router": "^15.2.3",
    "@angular/service-worker": "^15.2.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "dexie": "^2.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "google-auth-library": "^5.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.2",
    "path": "^0.12.7",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "web-push": "^3.4.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.4",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.3",
    "@angular/language-service": "^15.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~13.13.5",
    "codecov": "^3.6.1",
    "codelyzer": "^6.0.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "1.7",
    "protractor": "~7.0.0",
    "reload": "^3.0.3",
    "sass-planifolia": "^0.6.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.6.4"
  }
}
