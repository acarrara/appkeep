{
  "name": "appkeep",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node scripts/server.js",
    "preserve": "node scripts/credentials.js",
    "serve": "ng serve",
    "prebuild": "node scripts/credentials.js",
    "build": "ng build --configuration production",
    "preserve:prod": "npm run build",
    "serve:prod": "http-server -p 4000 --proxy http://localhost:3000 dist/appkeep",
    "test": "vitest",
    "test:coverage": "vitest --watch=false --coverage.enabled=true",
    "test:ci": "vitest --watch=false --coverage.enabled=true && codecov",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "postinstall": "npm run prebuild && ng build --configuration production",
    "icons": "reload -b -s icons.html -e svg,html -d src/ -p 9000",
    "prepatterns": "sass src/styles.scss src/patterns.css -I . --no-source-map",
    "patterns": "reload -b -s patterns.html -e svg,css,html -d src/ -p 9001"
  },
  "engines": {
    "node": "20.x",
    "npm": "~6.9.0"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "2.1",
    "@angular/animations": "^20.0.6",
    "@angular/common": "^20.0.6",
    "@angular/compiler": "^20.0.6",
    "@angular/core": "^20.0.6",
    "@angular/forms": "^20.0.6",
    "@angular/platform-browser": "^20.0.6",
    "@angular/platform-browser-dynamic": "^20.0.6",
    "@angular/pwa": "^0.803.16",
    "@angular/router": "^20.0.6",
    "@angular/service-worker": "^20.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "dexie": "^2.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "google-auth-library": "^5.5.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.2",
    "path": "^0.12.7",
    "rxjs": "7.8.2",
    "tslib": "^2.0.0",
    "vitest": "^3.2.4",
    "web-push": "^3.4.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.19.0",
    "@analogjs/vite-plugin-angular": "^1.19.0",
    "@analogjs/vitest-angular": "^1.19.0",
    "@angular-devkit/build-angular": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.6",
    "@angular/language-service": "^20.0.6",
    "@nx/angular": "^21.2.2",
    "@nx/devkit": "^21.2.2",
    "@nx/vite": "^21.2.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "angular-eslint": "20.1.1",
    "codecov": "^3.6.1",
    "codelyzer": "^6.0.0",
    "eslint": "^9.29.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsdom": "^22.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "protractor": "~7.0.0",
    "reload": "^3.0.3",
    "sass-planifolia": "^0.6.0",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "vite": "^7.0.2",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
